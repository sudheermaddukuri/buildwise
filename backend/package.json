{
  "name": "customhome-backend",
  "version": "0.1.0",
  "private": true,
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build:frontend": "npm --prefix ../frontend run build",
    "build:marketing": "npm --prefix ../marketing run build",
    "build:public": "npm run build:marketing && npm run build:frontend",
    "package:zip": "bash -lc 'cd .. && rm -f buildwise_server.zip && zip -r buildwise_server.zip backend -x \"backend/node_modules/**\"'",
    "lint": "echo \"No linter configured\"",
    "seed:templates": "node src/scripts/seedTemplates.js",
    "seed:permits": "node src/scripts/seedPermitsDallas.js"
  },
  "dependencies": {
    "express-async-errors": "^3.1.1",
    "aws-sdk": "^2.1692.0",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "openai": "^4.67.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}


